<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.example.myboot07.dao.ILoginDao">


<insert id="saceUserRegister" parameterType="com.example.myboot07.bean.UserRegister">
        insert into user (account,password,name,acatar_url,gmt_creat)
        values (#{userRegister.account},#{userRegister.password},#{userRegister.name},
        #{userRegister.acatar_url},#{userRegister.gmt_creat});
    </insert>
</mapper>